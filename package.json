{
    "name": "react-mf",
    "version": "0.0.1",
    "description": "A react component to display molecular formulas",
    "homepage": "",
    "author": {
        "name": "Daniel Kostro",
        "email": "kostro.d@gmail.com"
    },
    "files": [
        "lib",
        "lib-es6"
    ],
    "main": "lib/index.js",
    "@std/esm": "js",
    "keywords": [
        "react",
        "molecular formula"
    ],
    "module": "lib-es6/index.js",
    "scripts": {
        "compile": "babel src --out-dir lib --ignore __tests__ --quiet",
        "compile-es6": "BABEL_ENV=es6 babel src --out-dir lib-es6 --ignore __tests__ --quiet",
        "start": "webpack-dev-server",
        "test": "jest",
        "eslint": "eslint src",
        "eslint-fix": "eslint --fix src",
        "prepublishOnly": "run-p compile compile-es6"
    },
    "peerDependencies": {
        "react": ">=16.0.0",
        "react-dom": ">=16.0.0"
    },
    "devDependencies": {
        "@std/esm": "^0.16.0",
        "babel-cli": "^6.22.2",
        "babel-core": "^6.22.1",
        "babel-loader": "^6.2.10",
        "babel-preset-env": "^1.1.8",
        "babel-preset-react": "^6.22.0",
        "eslint": "^3.14.1",
        "eslint-config-cheminfo": "^1.6.0",
        "eslint-plugin-no-only-tests": "^1.1.0",
        "eslint-plugin-react": "^6.9.0",
        "jest-cli": "^18.1.0",
        "npm-run-all": "^4.0.1",
        "react": "^16.1.1",
        "react-dom": "^16.1.1",
        "react-test-renderer": "^15.4.2",
        "webpack": "^1.14.0",
        "webpack-dev-server": "^1.16.2"
    },
    "repository": "neptunjs/react-mf",
    "license": "MIT"
}
